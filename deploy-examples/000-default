<VirtualHost *:80>
	ServerAdmin webmaster@localhost
	ServerName localhost

	WSGIDaemonProcess patokah processes=2 threads=15
	WSGIProcessGroup patokah

	WSGIScriptAlias /patokah /var/www/patokah/patokah.wsgi

	<Directory /var/www/patokah>
		WSGIProcessGroup patokah
		WSGIApplicationGroup %{GLOBAL}
		Order deny,allow
		Allow from all
	</Directory>
</VirtualHost>
